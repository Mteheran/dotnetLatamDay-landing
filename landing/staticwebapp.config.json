{
"navigationFallback": {
      "rewrite": "index.html",
      "exclude": ["/images/*.{png,jpg,gif}", "/css/*", "/images/*", "/js/*"]
    },
"responseOverrides": {     
      "404": {
        "rewrite": "/index.html",
        "statusCode": 200
      }
    },
"mimeTypes": {
        ".json": "text/json",
        ".css": "text/css"
  }
}
